#include<stdio.h>

int main()
{ 
   int a,b,m,g,f;
   printf("Enter first number A=");
   scanf("%d",&a);
   printf("Enter second number B=");
   scanf("%d",&b);
   m=a+b;
   g=a-b;
   f=a*b;
   printf("Sum=%d",m);
   printf("\n");
   printf("subtract=%d",g);
   printf("%\n");
   printf("multiply=%d",f);
   
   }
